# Copy this file to .env and fill the values

# PostgreSQL connection string
DATABASE_URL="postgresql://user:password@host:5432/dbname?schema=public"

# JWT configuration
JWT_SECRET_KEY="your-strong-secret"
# Optional: e.g. 1h, 2d
JWT_EXPIRATION="1h"

# Logging
# LOG_LEVEL can be: error, warn, info, http, verbose, debug, silly
LOG_LEVEL="info"
# LOG_TO_FILES should remain unset/false on Vercel (filesystem is ephemeral)
LOG_TO_FILES="false"
