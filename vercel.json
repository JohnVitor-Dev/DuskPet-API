{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(?:html|css|js|png|jpg|jpeg|gif|svg|ico|webp|txt|map))",
            "dest": "/public/$1"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}